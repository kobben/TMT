{
  
  "width": 700,
  "height": 580,
  "padding": 0,
  "config": {
    "view": {
      "stroke": "none"
    },
    "legend": {
      "orient": "right",
      "titlePadding": 15,
      "titleFontSize": 15,
      "titleFontWeight": "bold",
      "labelOffset": 20,
      "symbolSize": 300,
      "symbolStrokeWidth": 0.0,
      "symbolType": "M-1,1h3v-1h-3Z"
    }
  },
  "data": {
    "url": "%DATA_URL%_buffer",
    "format": {
      "type": "json",
      "property": "%DATA_PROPERTY%"
    }
  },
  "projection": {
    "type": "stereographic"
  },
  "layer": [
    {
      "mark": {
        "type": "geoshape",
        "stroke": "black",
        "strokeWidth": 0.0
      },
      "encoding": {
        "color": {
          "field": "%DATA_ATTRIBUTE%",
          "type": "ordinal",
          "scale": {
            "range": [
              "url(#dots-1)",
              "url(#dots-2)",
              "url(#dots-3)",
              "url(#dots-4)",
              "url(#dots-5)",
              "url(#dots-6)",
              "url(#dots-7)",
              "url(#dots-8)",
              "url(#dots-9)"
            ]
          },
          "legend": {
            "title": "%LEGEND_TITLE%"
          }
        }
      }
    }
  ]
}





